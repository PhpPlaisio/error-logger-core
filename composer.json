{
  "name": "setbased/abc-error-logger-core",
  "description": "ABC-Framework: Core Error Logger",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "ABC-Framework",
    "Error",
    "Logger",
    "Core"
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-mbstring": "*",
    "setbased/abc-error-logger": "^2.0.0",
    "setbased/abc-html": "^2.0.0",
    "setbased/abc-var-dumper": "^2.0.0",
    "setbased/abc-var-dumper-writer": "^2.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\ErrorLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\ErrorLogger\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
