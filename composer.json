{
  "name": "plaisio/error-logger-core",
  "description": "PhpPlaisio: Core Error Logger",
  "keywords": [
    "PhpPlaisio",
    "Error",
    "Logger",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-mbstring": "*",
    "plaisio/error-logger": "^3.0.0",
    "plaisio/html": "^3.0.0",
    "plaisio/var-dumper": "^3.0.0",
    "plaisio/var-dumper-writer": "^4.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ErrorLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\ErrorLogger\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
