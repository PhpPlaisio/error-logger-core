{
  "name": "plaisio/error-logger-core",
  "description": "PhpPlaisio: Core Error Logger",
  "keywords": [
    "PhpPlaisio",
    "Error Logger",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*",
    "plaisio/error-logger": "^1.4.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/var-dumper": "^1.6.0",
    "plaisio/var-dumper-writer": "^3.2.0"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ErrorLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\ErrorLogger\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
