{
  "name": "plaisio/error-logger-core",
  "description": "PhpPlaisio: Core Error Logger",
  "keywords": [
    "PhpPlaisio",
    "Error Logger",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-mbstring": "*",
    "plaisio/error-logger": "^1.2.0",
    "plaisio/helper-html": "^4.0.0",
    "plaisio/var-dumper": "^1.3.1",
    "plaisio/var-dumper-writer": "^2.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.17.0",
    "phpunit/phpunit": "^9.5.11"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ErrorLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\ErrorLogger\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
